Section:NewToggle("Auto Buy A (1,000)", "Buys A once", function(state)
    if state then
        _G.on = true
        while _G.on do
            wait()
            local args = {
                [1] = "A"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
        end
    else
        _G.on = false
    end
end)

