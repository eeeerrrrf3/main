  	end
})

Tab:AddButton({
	Name = "Universal Eat Self Hats",
	Callback = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/eeeerrrrf3/main/main/Universalallgame/eat%20hat'))()
  	end
